                            &lt;a href=&quot;https://itunes.apple.com/us/podcast/dan-carlins-hardcore-history/id173001861?mt=2&amp;uo=2&quot;&gt;&lt;img border=&quot;0&quot; alt=&quot;Dan Carlin - Dan Carlin's Hardcore History  artwork&quot; src=&quot;http://is5.mzstatic.com/image/thumb/Podcasts7/v4/88/ff/38/88ff38bb-3c07-dac3-200b-0bac260681ae/mza_6421670988208781371.jpg/170x170bb-85.jpg&quot; /&gt;&lt;/a&gt;
                        &lt;/td&gt;
                        &lt;td width=&quot;10&quot;&gt;&lt;img alt=&quot;&quot; width=&quot;10&quot; height=&quot;1&quot; src=&quot;https://s.mzstatic.com/images/spacer.gif&quot; /&gt;&lt;/td&gt;
                    	&lt;td width=&quot;95%&quot;&gt;
                    
                    
                        &lt;b&gt;&lt;a href=&quot;https://itunes.apple.com/us/podcast/dan-carlins-hardcore-history/id173001861?mt=2&amp;uo=2&quot;&gt;Dan Carlin's Hardcore History&lt;/a&gt;&lt;/b&gt;&lt;br/&gt;
                        
                        
                        
                        

                        &lt;a href=&quot;https://itunes.apple.com/us/artist/wizzard-media/id256201037?mt=2&amp;amp;uo=2&quot;&gt;Dan Carlin&lt;/a&gt;

                       &lt;font size=&quot;2&quot; face=&quot;Helvetica,Arial,Geneva,Swiss,SunSans-Regular&quot;&gt;
						
						 	&lt;br/&gt;
                            &lt;b&gt;Genre:&lt;/b&gt; &lt;a href=&quot;https://itunes.apple.com/us/genre/podcasts-history/id1462?mt=2&amp;uo=2&quot;&gt;History&lt;/a&gt;
                        
